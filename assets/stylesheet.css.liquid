.cursorAnimate {
            position: absolute;
            left: 150px;
            top: 25px;
            opacity: 0;
            transform: scale(1);
            animation: moveAndFade 4s ease-in-out infinite;
            animation-delay: 1s;
        }
.glow {
      display: inline-block;
      -webkit-animation: glow 1s ease-in-out infinite alternate;
      -moz-animation: glow 1s ease-in-out infinite alternate;
      animation: glow 1s ease-in-out infinite alternate;
      -webkit-transform-origin: center;
      -moz-transform-origin: center;
      -ms-transform-origin: center;
      -o-transform-origin: center;
      transform-origin: center;
    }

    @-webkit-keyframes glow {
      from {
        text-shadow: 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200;
        -webkit-transform: scale(1);
        transform: scale(1);
      }
      to {
        text-shadow: 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 1 3px #FCE200;
        -webkit-transform: scale(1.2);
        transform: scale(1.2);
      }
    }

    @-moz-keyframes glow {
      from {
        text-shadow: 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200;
        -moz-transform: scale(1);
        transform: scale(1);
      }
      to {
        text-shadow: 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 1 3px #FCE200;
        -moz-transform: scale(1.2);
        transform: scale(1.2);
      }
    }

    @keyframes glow {
      from {
        text-shadow: 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200;
        transform: scale(1);
      }
      to {
        text-shadow: 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 0 3px #FCE200, 0 1 3px #FCE200;
        transform: scale(1.2);
      }
    }
    @keyframes moveAndFade {
            80% {
                opacity: 0;
                transform: scale(1);
            }
            83% {
                opacity: 1;
                transform: scale(1);
            }
            86% {
                opacity: 1;
                transform: scale(1.2);
            }
            89% {
                opacity: 1;
                transform: scale(1);
            }
            92% {
                opacity: 1;
                transform: scale(1.2);
            }
            100% {
                opacity: 1;
                transform: scale(1);
            }
        }

