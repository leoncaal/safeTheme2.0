.loaderVIN {
  width: 48px;
  height: 48px;
  display: block;
  margin: 20px auto;
  position: relative;
  border: 3px solid #292929;
  border-radius: 50%;
  box-sizing: border-box;
  animation: animloaderVIN 2s linear infinite;
}
.loaderVIN::after {
  content: '';  
  box-sizing: border-box;
  width: 6px;
  height: 24px;
  background: #292929;
  transform: rotate(-45deg);
  position: absolute;
  bottom: -20px;
  left: 46px;
}

@keyframes animloaderVIN {
  0% {
    transform: translate(-10px, -10px);
  }
  25% {
    transform: translate(-10px, 10px);
  }
  50% {
    transform: translate(10px, 10px);
  }
  75% {
    transform: translate(10px, -10px);
  }
  100% {
    transform: translate(-10px, -10px);
  }
}
