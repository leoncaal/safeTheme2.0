#overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 1); /* Fondo semi-transparente */
  z-index: 9999;
  display: flex; /* Ocultar por defecto */
   visibility: visible;
}
#overlay2 {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0); /* Fondo semi-transparente */
  z-index: 9998;
  display: none; /* Ocultar por defecto */
  visibility: visible;
}
@media only screen and (max-width: 767px) {
